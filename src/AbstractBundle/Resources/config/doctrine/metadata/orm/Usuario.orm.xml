<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuario">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
      <unique-constraint name="email_UNIQUE" columns="str_email"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="strEmail" type="string" column="str_email" length="155" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="strSenha" type="string" column="str_senha" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
