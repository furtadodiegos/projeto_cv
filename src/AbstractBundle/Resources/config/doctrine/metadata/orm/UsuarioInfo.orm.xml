<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsuarioInfo" table="usuario_info">
    <indexes>
      <index name="fk_usuario_info_usuario_idx" columns="usuario_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="strNome" type="string" column="str_nome" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="strSobrenome" type="string" column="str_sobrenome" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datNascimento" type="date" column="dat_nascimento" nullable="true"/>
    <many-to-one field="usuario" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
