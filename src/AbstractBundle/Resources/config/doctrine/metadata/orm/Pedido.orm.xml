<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pedido" table="pedido">
    <indexes>
      <index name="fk_pedido_usuario1_idx" columns="usuario_id"/>
      <index name="fk_pedido_carro1_idx" columns="carro_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datPedido" type="datetime" column="dat_pedido" nullable="false"/>
    <many-to-one field="carro" target-entity="Carro" fetch="LAZY">
      <join-columns>
        <join-column name="carro_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="usuario" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
